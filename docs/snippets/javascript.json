{

    "req": {
        "prefix": "req",
        "body": [
            "req({",
            "    endpoint: \"$1\"",
            "}).then(res => {",
            "    this.$2 = res;",
            "}).catch(error => {",
            "    console.error('Error:', error);",
            "});"
        ],
        "description": "Promise-based req snippet with error handling"
    },
    "req_await": {
        "prefix": "req_await",
        "body": [
            "this.$1 = await req({",
            "    endpoint: \"$2\"",
            "});"
        ],
        "description": "Async/await req snippet"
    },


